###  http://bimerr.iot.linkeddata.es/def/building#hasApartment
:hasApartment rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf bot:containsZone ;
              rdfs:comment "Relation to indicate that a storey has apartments."@en ;
              rdfs:label "has apartment"@en .


###  http://bimerr.iot.linkeddata.es/def/building#hasLayer
:hasLayer rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf bot:hasSubElement ;
          rdfs:comment "Relation to link construction components (walls, floors, etc) with their internal layers."@en ;
          rdfs:label "has layer"@en .


###  http://bimerr.iot.linkeddata.es/def/building#hasSubComponent
:hasSubComponent rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf bot:hasSubElement ;
                 rdfs:comment "Relation to link building components (HVAC, doors, etc) with the sub-components they are conformed with."@en ;
                 rdfs:label "has subcomponent"@en .


###  http://bimerr.iot.linkeddata.es/def/building#includesMaterial
:includesMaterial rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf bot:hasSubElement ;
                  rdfs:range :Material ;
                  rdfs:label "includes material"@en .


###  https://w3id.org/bot#containsZone
bot:containsZone rdf:type owl:ObjectProperty ;
                 rdfs:domain bot:Zone ;
                 rdfs:range bot:Zone ;
                 rdfs:comment "Relation to buildings contained in a zone. The typical domains of bot:hasBuilding are instances of bot:Site."@en ;
                 rdfs:isDefinedBy <http://w3id.org/bot#> ;
                 rdfs:label "contains zone"@en .


###  https://w3id.org/bot#hasElement
bot:hasElement rdf:type owl:ObjectProperty ;
               rdfs:domain bot:Zone ;
               rdfs:range bot:Element ;
               rdfs:comment "Links a Zone to an Element that is contained in, or adjacent to, the Zone. The intended use of this relationship is not to be stated explicitly, but to be inferred from its sub-properties. It will, for example, allow one to query for all the doors of a building given that they have an adjacency to spaces of the building."@en ;
               rdfs:isDefinedBy <http://w3id.org/bot#> ;
               rdfs:label "has element"@en .


###  https://w3id.org/bot#hasSpace
bot:hasSpace rdf:type owl:ObjectProperty ;
             rdfs:domain bot:Zone ;
             rdfs:range bot:Space ;
             rdfs:comment "Relation to spaces contained in a zone. The typical domains of bot:hasSpace are instances of bot:Storey and bot:Building."@en ;
             rdfs:isDefinedBy <http://w3id.org/bot#> ;
             rdfs:label "has space"@en .


###  https://w3id.org/bot#hasStorey
bot:hasStorey rdf:type owl:ObjectProperty ;
              rdfs:domain bot:Zone ;
              rdfs:range bot:Storey ;
              rdfs:comment "Relation to storeys contained in a zone. The typical domains of bot:hasStorey are instances of bot:Building."@en ;
              rdfs:isDefinedBy <http://w3id.org/bot#> ;
              rdfs:label "has storey"@en .


###  https://w3id.org/bot#hasSubElement
bot:hasSubElement rdf:type owl:ObjectProperty ;
                  rdfs:domain bot:Element ;
                  rdfs:range bot:Element ;
                  rdfs:comment "Relation between an element a) and another element b) hosted by element a)."@en ;
                  rdfs:isDefinedBy <http://w3id.org/bot#> ;
                  rdfs:label "has sub-element"@en .


