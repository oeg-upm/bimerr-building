###  http://bimerr.iot.linkeddata.es/def/building#Apartment
:Apartment rdf:type owl:Class ;
           rdfs:subClassOf bot:Zone ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty bot:hasSpace ;
                             owl:allValuesFrom :Space
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty bot:hasSimple3DModel ;
                             owl:allValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :ifcIdentifier ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] ;
           rdfs:comment "A delimited zone within a storey which brings shelter to its residents."@en ;
           rdfs:label "Apartment"@en ;
           rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcZone"^^xsd:string .


