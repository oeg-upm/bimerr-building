# 
# http://bimerr.iot.linkeddata.es/def/building#buildingAddress

:buildingAddress a owl:DatatypeProperty ;
	rdfs:domain :Building ;
	rdfs:range xsd:string ;
	rdfs:comment "Address of the building"@en ;
	rdfs:label "building address"@en .
# 
# http://bimerr.iot.linkeddata.es/def/building#hasSimple3DModel

:hasSimple3DModel a owl:DatatypeProperty ;
	rdfs:label "has simple 3D model"@en .
# 
# http://bimerr.iot.linkeddata.es/def/building#ifcIdentifier

:ifcIdentifier a owl:DatatypeProperty ;
	rdfs:range xsd:string ;
	rdfs:comment "Identifier within the IFC file."@en ;
	rdfs:label "ifc identifier"@en .
# 
# http://bimerr.iot.linkeddata.es/def/building#maxOccupants

:maxOccupants a owl:DatatypeProperty ;
	rdfs:range xsd:integer ;
	rdfs:comment "Maximum limit range to describe the number of occupants inside a zone."@en ;
	rdfs:label "max number occupants"@en .
# 
# http://bimerr.iot.linkeddata.es/def/building#minOccupants

:minOccupants a owl:DatatypeProperty ;
	rdfs:range xsd:integer ;
	rdfs:comment "Minimum limit range to describe the number of occupants inside a zone."@en ;
	rdfs:label "min number occupants"@en .
# 
# http://www.w3.org/2003/01/geo/wgs84_pos#alt

geo:alt a owl:DatatypeProperty ;
	rdfs:comment "Attribute to indicate the altitud geolocalization coordinate of a building."@en ;
	rdfs:isDefinedBy <http://www.w3.org/2003/01/geo/wgs84_pos#> ;
	rdfs:label "altitud"@en .
# 
# http://www.w3.org/2003/01/geo/wgs84_pos#lat

geo:lat a owl:DatatypeProperty ;
	rdfs:comment "Attribute to indicate the latitud geolocalization coordinate of a building."@en ;
	rdfs:isDefinedBy <http://www.w3.org/2003/01/geo/wgs84_pos#> ;
	rdfs:label "latitud"@en .
# 
# http://www.w3.org/2003/01/geo/wgs84_pos#long

geo:long a owl:DatatypeProperty ;
	rdfs:comment "Attribute to indicate the longitud geolocalization coordinate of a building."@en ;
	rdfs:isDefinedBy <http://www.w3.org/2003/01/geo/wgs84_pos#> ;
	rdfs:label "longitud"@en .
