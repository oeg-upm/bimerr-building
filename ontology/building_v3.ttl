@prefix : <http://bimerr.iot.linkeddata.es/def/building#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix bot: <https://w3id.org/bot#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix props: <http://bimerr.iot.linkeddata.es/def/material-properties#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix building: <http://bimerr.iot.linkeddata.es/def/building#> .
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@base <http://bimerr.iot.linkeddata.es/def/building#> .

<http://bimerr.iot.linkeddata.es/def/building#> rdf:type owl:Ontology ;
                                                 building:preferredNamespacePrefix "building" ;
                                                 dc:creator "María Poveda-Villalón" ,
                                                            "Serge Chávez-Feria" ,
                                                            "The Building ontology has been developed to perform as the core module of the BIMERR project, containing information related to buidling topology, components, materials, and geometry. The geometry information is managed as information objects whitout entering in the details of the BIM model."@en ;
                                                 dc:publisher "http://purl.org/NET/rdflicense/cc-by4.0" ,
                                                              "http://www.oeg-upm.net/" ;
                                                 dc:title "Building Ontology" ;
                                                 owl:versionInfo "0.0.3" .

#################################################################
#    Annotation properties
#################################################################

###  http://bimerr.iot.linkeddata.es/def/building#preferredNamespacePrefix
building:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/license
dc:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/publisher
dc:publisher rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://bimerr.iot.linkeddata.es/def/building#hasApartment
building:hasApartment rdf:type owl:ObjectProperty ;
                      rdfs:comment "Relation between a storey and the apartments it is composed of."@en ;
                      rdfs:label "has apartment" .


###  http://bimerr.iot.linkeddata.es/def/building#hasConstruction
building:hasConstruction rdf:type owl:ObjectProperty ;
                         rdfs:comment "Relation between a building element and its construction. For example a window has two construction individuals: The Lining and the glazing."@en ;
                         rdfs:label "has construction" .


###  http://bimerr.iot.linkeddata.es/def/building#hasMaterial
building:hasMaterial rdf:type owl:ObjectProperty ;
                     rdfs:comment "Relation between a construction component (Layer, profile, constituent) and the materials that define them."@en ;
                     rdfs:label "has material" .


###  http://bimerr.iot.linkeddata.es/def/building#hasMaterialConstituent
building:hasMaterialConstituent rdf:type owl:ObjectProperty ;
                                rdfs:comment "A relation between a constituent set and the specific material constituents."@en ;
                                rdfs:label "has material constituent" .


###  http://bimerr.iot.linkeddata.es/def/building#hasMaterialLayer
building:hasMaterialLayer rdf:type owl:ObjectProperty ;
                          rdfs:comment "A relation between a layer set and the specific material layers."@en ;
                          rdfs:label "has material layer" .


###  http://bimerr.iot.linkeddata.es/def/building#hasMaterialProfile
building:hasMaterialProfile rdf:type owl:ObjectProperty ;
                            rdfs:comment "A relation between a profile set and the specific material profiles."@en ;
                            rdfs:label "has material profile" .


###  http://bimerr.iot.linkeddata.es/def/building#includesElement
building:includesElement rdf:type owl:ObjectProperty ;
                         rdfs:comment "Relationship that connects a system with an element or device."@en ;
                         rdfs:label "includes element" .


###  http://bimerr.iot.linkeddata.es/def/building#isPartOfSystem
building:isPartOfSystem rdf:type owl:ObjectProperty ;
                        rdfs:comment "Relationship that indicates that an element or device is part of a system."@en ;
                        rdfs:label "is part of system" .


###  https://w3id.org/bot#containsZone
bot:containsZone rdf:type owl:ObjectProperty ;
                 rdfs:comment "Relationship to the subzones of a major zone. A space zone could for instance be contained in a storey zone which is further contained in a building zone."@en ;
                 rdfs:label "contains zone" .


###  https://w3id.org/bot#hasElement
bot:hasElement rdf:type owl:ObjectProperty ;
               rdfs:comment "Links a Zone to an Element that is contained in, or adjacent to, the Zone."@en ;
               rdfs:label "has element" .


###  https://w3id.org/bot#hasSpace
bot:hasSpace rdf:type owl:ObjectProperty ;
             rdfs:comment "Relation to spaces contained in a zone."@en ;
             rdfs:label "has space" .


###  https://w3id.org/bot#hasStorey
bot:hasStorey rdf:type owl:ObjectProperty ;
              rdfs:comment "Relation to storeys contained in a zone."@en ;
              rdfs:label "has storey" .


###  https://w3id.org/bot#hasSubElement
bot:hasSubElement rdf:type owl:ObjectProperty ;
                  rdfs:comment "Relationship to indicate the subcomponents of elements"@en ;
                  rdfs:label "has sub element" .


#################################################################
#    Data properties
#################################################################

###  http://bimerr.iot.linkeddata.es/def/building#buildingAddress
building:buildingAddress rdf:type owl:DatatypeProperty ;
                         rdfs:domain building:Building ;
                         rdfs:range xsd:string ;
                         rdfs:comment "The postal address of the building"@en ;
                         rdfs:label "building address" .


###  http://bimerr.iot.linkeddata.es/def/building#ifcIdentifier
building:ifcIdentifier rdf:type owl:DatatypeProperty ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Identifier used in the IFC model (Global Unique ID)."@en ;
                       rdfs:label "ifc identifier" .


###  http://bimerr.iot.linkeddata.es/def/building#maxOccupants
building:maxOccupants rdf:type owl:DatatypeProperty ;
                      rdfs:range xsd:integer ;
                      rdfs:comment "Maximum number of occupants inside a building."@en ;
                      rdfs:label "max occupants" .


###  http://bimerr.iot.linkeddata.es/def/building#minOccupants
building:minOccupants rdf:type owl:DatatypeProperty ;
                      rdfs:range xsd:integer ;
                      rdfs:comment "Minimum number of occupants inside a building."@en ;
                      rdfs:label "min occupants" .


###  http://bimerr.iot.linkeddata.es/def/building#position
building:position rdf:type owl:DatatypeProperty ;
                  rdfs:range xsd:integer ;
                  rdfs:comment "Position of a layer with respect to its construction (MaterialLayerSet). The first layer is the one facing the outdoors."@en ;
                  rdfs:label "position" .


###  http://bimerr.iot.linkeddata.es/def/building#thickness
building:thickness rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:float ;
                   rdfs:comment "Thickness of a layer."@en ;
                   rdfs:label "thickness" .


###  http://www.w3.org/2003/01/geo/wgs84_pos#alt
wgs84_pos:alt rdf:type owl:DatatypeProperty ;
              rdfs:comment "Altitude geo location coordinate."@en ;
              rdfs:label "altitude"@en .


###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
wgs84_pos:lat rdf:type owl:DatatypeProperty ;
              rdfs:comment "Latitude geo location coordinate."@en ;
              rdfs:label "latitude"@en .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
wgs84_pos:long rdf:type owl:DatatypeProperty ;
               rdfs:comment "Longitude geo location coordinate."@en ;
               rdfs:label "longitude"@en .


###  https://w3id.org/bot#hasSimple3DModel
bot:hasSimple3DModel rdf:type owl:DatatypeProperty ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Links any Zone or Element to a 3D Model encoded as a literal."@en ;
                     rdfs:label "has simple 3d model" .


#################################################################
#    Classes
#################################################################

###  http://bimerr.iot.linkeddata.es/def/building#Apartment
building:Apartment rdf:type owl:Class ;
                   rdfs:subClassOf bot:Zone ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty bot:hasSpace ;
                                     owl:allValuesFrom building:Space
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty building:ifcIdentifier ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onDataRange xsd:string
                                   ] ;
                   rdfs:comment "An apartment is a group of spaces, partial spaces or other zones."@en ;
                   rdfs:label "Apartment" ;
                   rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcZone" .


###  http://bimerr.iot.linkeddata.es/def/building#BiomassBoiler
building:BiomassBoiler rdf:type owl:Class ;
                       rdfs:subClassOf building:Boiler ;
                       rdfs:label "Biomass Boiler" .


###  http://bimerr.iot.linkeddata.es/def/building#Boiler
building:Boiler rdf:type owl:Class ;
                rdfs:subClassOf building:EnergyConversionDevice ;
                rdfs:comment "A boiler is a closed, pressure-rated vessel in which water or other fluid is heated using an energy source such as natural gas, heating oil, or electricity. The fluid in the vessel is then circulated out of the boiler for use in various processes or heating applications."@en ;
                rdfs:label "Boiler" ;
                rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcBoiler"@en .


###  http://bimerr.iot.linkeddata.es/def/building#Building
building:Building rdf:type owl:Class ;
                  rdfs:subClassOf bot:Building ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty bot:hasStorey ;
                                    owl:allValuesFrom building:Storey
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty building:ifcIdentifier ;
                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onDataRange xsd:string
                                  ] ;
                  rdfs:comment "A building represents a structure that provides shelter for its occupants or contents and stands in one place. The building is also used to provide a basic element within the spatial structure hierarchy for the components of a building project (together with site, storey, and space)."@en ;
                  rdfs:label "Building" ;
                  rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcBuilding" .


###  http://bimerr.iot.linkeddata.es/def/building#BuildingElement
building:BuildingElement rdf:type owl:Class ;
                         rdfs:subClassOf building:Element ;
                         rdfs:comment "The building element comprises all elements that are primarily part of the construction of a building, i.e., its structural and space separating system. Building elements are all physically existent and tangible things."@en ;
                         rdfs:label "Building Element" ;
                         rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcBuildingElement" .


###  http://bimerr.iot.linkeddata.es/def/building#CoolingSystem
building:CoolingSystem rdf:type owl:Class ;
                       rdfs:subClassOf building:System ;
                       rdfs:comment "System that aggregates cooling devices."@en ;
                       rdfs:label "Cooling System" .


###  http://bimerr.iot.linkeddata.es/def/building#DistributionElement
building:DistributionElement rdf:type owl:Class ;
                             rdfs:subClassOf building:Element ;
                             rdfs:comment "A generalization of all elements that participate in a distribution system."@en ;
                             rdfs:label "Distribution Element" ;
                             rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcDistributionElement"@en .


###  http://bimerr.iot.linkeddata.es/def/building#DistributionFlowElement
building:DistributionFlowElement rdf:type owl:Class ;
                                 rdfs:subClassOf building:DistributionElement ;
                                 rdfs:comment "Defines occurrence elements of a distribution system that facilitate the distribution of energy or matter, such as air, water or power."@en ;
                                 rdfs:label "Distribution Flow Element" ;
                                 rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcDistributionFlowElement" .


###  http://bimerr.iot.linkeddata.es/def/building#Door
building:Door rdf:type owl:Class ;
              rdfs:subClassOf building:BuildingElement ;
              rdfs:comment "The door is a building element that is predominately used to provide controlled access for people and goods. It includes constructions with hinged, pivoted, sliding, and additionally revolving and folding operations. A door consists of a lining and one or several panels."@en ;
              rdfs:label "Door" ;
              rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcDoor" .


###  http://bimerr.iot.linkeddata.es/def/building#DuctSegment
building:DuctSegment rdf:type owl:Class ;
                     rdfs:subClassOf building:FlowSegment ;
                     rdfs:comment "A duct segment is used to typically join two sections of duct network."@en ;
                     rdfs:label "Duct Segment" ;
                     rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcDuctSegment" .


###  http://bimerr.iot.linkeddata.es/def/building#Element
building:Element rdf:type owl:Class ;
                 rdfs:subClassOf bot:Element ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty building:hasConstruction ;
                                   owl:allValuesFrom [ rdf:type owl:Class ;
                                                       owl:unionOf ( building:MaterialConstituentSet
                                                                     building:MaterialLayerSet
                                                                     building:MaterialProfileSet
                                                                   )
                                                     ]
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty bot:hasSubElement ;
                                   owl:allValuesFrom building:Element
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty building:isPartOfSystem ;
                                   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass building:System
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty building:ifcIdentifier ;
                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onDataRange xsd:string
                                 ] ;
                 rdfs:comment "An element is a generalization of all components that make up an AEC product."@en ;
                 rdfs:label "Element" ;
                 rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcElement" .


###  http://bimerr.iot.linkeddata.es/def/building#EnergyConversionDevice
building:EnergyConversionDevice rdf:type owl:Class ;
                                rdfs:subClassOf building:DistributionFlowElement ;
                                rdfs:comment "Defines the occurrence of a device used to perform energy conversion or heat transfer and typically participates in a flow distribution system."@en ;
                                rdfs:label "Energy Conversion Device" ;
                                rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcEnergyConversionDevice" .


###  http://bimerr.iot.linkeddata.es/def/building#Fan
building:Fan rdf:type owl:Class ;
             rdfs:subClassOf building:FlowMovingDevice ;
             rdfs:comment "A fan is a device which imparts mechanical work on a gas. A typical usage of a fan is to induce airflow in a building services air distribution system."@en ;
             rdfs:label "Fan" ;
             rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcFan" .


###  http://bimerr.iot.linkeddata.es/def/building#FenestrationSystem
building:FenestrationSystem rdf:type owl:Class ;
                            rdfs:subClassOf building:System ;
                            rdfs:comment "System that aggregates fenestration elements."@en ;
                            rdfs:label "Fenestration System" .


###  http://bimerr.iot.linkeddata.es/def/building#FlowController
building:FlowController rdf:type owl:Class ;
                        rdfs:subClassOf building:DistributionFlowElement ;
                        rdfs:comment "Defines the occurrence of elements of a distribution system that are used to regulate flow through a distribution system. Examples include dampers, valves, switches, and relays."@en ;
                        rdfs:label "Flow Controller" ;
                        rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcFlowController" .


###  http://bimerr.iot.linkeddata.es/def/building#FlowMovingDevice
building:FlowMovingDevice rdf:type owl:Class ;
                          rdfs:subClassOf building:DistributionFlowElement ;
                          rdfs:comment "Defines the occurrence of an apparatus used to distribute, circulate or perform conveyance of fluids, including liquids and gases (such as a pump or fan), and typically participates in a flow distribution system."@en ;
                          rdfs:label "Flow Moving Device" ;
                          rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcFlowMovingDevice" .


###  http://bimerr.iot.linkeddata.es/def/building#FlowSegment
building:FlowSegment rdf:type owl:Class ;
                     rdfs:subClassOf building:DistributionFlowElement ;
                     rdfs:comment "Defines the occurrence of a segment of a flow distribution system."@en ;
                     rdfs:label "Flow Segment" ;
                     rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcFlowSegment" .


###  http://bimerr.iot.linkeddata.es/def/building#FlowStorageDevice
building:FlowStorageDevice rdf:type owl:Class ;
                           rdfs:subClassOf building:DistributionFlowElement ;
                           rdfs:comment "Defines the occurrence of a device that participates in a distribution system and is used for temporary storage (such as a tank)."@en ;
                           rdfs:label "Flow Storage Device" ;
                           rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcFlowStorageDevice" .


###  http://bimerr.iot.linkeddata.es/def/building#FlowTerminal
building:FlowTerminal rdf:type owl:Class ;
                      rdfs:subClassOf building:DistributionFlowElement ;
                      rdfs:comment "Defines the occurrence of a permanently attached element that acts as a terminus or beginning of a distribution system (such as an air outlet, drain, water closet, or sink). A terminal is typically a point at which a system interfaces with an external environment."@en ;
                      rdfs:label "Flow Terminal" ;
                      rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcFlowTerminal" .


###  http://bimerr.iot.linkeddata.es/def/building#FossilBoiler
building:FossilBoiler rdf:type owl:Class ;
                      rdfs:subClassOf building:Boiler ;
                      rdfs:label "Fossil Boiler" .


###  http://bimerr.iot.linkeddata.es/def/building#HeatExchanger
building:HeatExchanger rdf:type owl:Class ;
                       rdfs:subClassOf building:EnergyConversionDevice ;
                       rdfs:comment "A heat exchanger is a device used to provide heat transfer between non-mixing media such as plate and shell and tube heat exchangers."@en ;
                       rdfs:label "Heat Exchanger" ;
                       rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcHeatExchanger"@en .


###  http://bimerr.iot.linkeddata.es/def/building#HeatingSystem
building:HeatingSystem rdf:type owl:Class ;
                       rdfs:subClassOf building:System ;
                       rdfs:comment "System that aggregates heating devices."@en ;
                       rdfs:label "Heating System" .


###  http://bimerr.iot.linkeddata.es/def/building#MaterialConstituent
building:MaterialConstituent rdf:type owl:Class ;
                             rdfs:subClassOf [ rdf:type owl:Restriction ;
                                               owl:onProperty building:hasMaterial ;
                                               owl:allValuesFrom props:Material
                                             ] ;
                             rdfs:comment "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcMaterialConstituent"@en ;
                             rdfs:label "Material Constituent" .


###  http://bimerr.iot.linkeddata.es/def/building#MaterialConstituentSet
building:MaterialConstituentSet rdf:type owl:Class ;
                                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                  owl:onProperty building:hasMaterialConstituent ;
                                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onClass building:MaterialConstituent
                                                ] ;
                                rdfs:comment "A single and identifiable part of an element which is constructed of a number of layers (one or more)."@en ;
                                rdfs:label "Material Constituent Set" ;
                                rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcMaterialConstituentSet" .


###  http://bimerr.iot.linkeddata.es/def/building#MaterialLayer
building:MaterialLayer rdf:type owl:Class ;
                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                         owl:onProperty building:hasMaterial ;
                                         owl:allValuesFrom props:Material
                                       ] ;
                       rdfs:comment "A single and identifiable part of an element which is constructed of a number of layers (one or more)."@en ;
                       rdfs:label "Material Layer" ;
                       rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcMaterialLayer" .


###  http://bimerr.iot.linkeddata.es/def/building#MaterialLayerSet
building:MaterialLayerSet rdf:type owl:Class ;
                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                            owl:onProperty building:hasMaterialLayer ;
                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                            owl:onClass building:MaterialLayer
                                          ] ;
                          rdfs:comment "A designation by which materials of an element constructed of a number of material layers is known and through which the relative positioning of individual layers can be expressed."@en ;
                          rdfs:label "Material Layer Set" ;
                          rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcMaterialLayerSet" .


###  http://bimerr.iot.linkeddata.es/def/building#MaterialProfile
building:MaterialProfile rdf:type owl:Class ;
                         rdfs:subClassOf [ rdf:type owl:Restriction ;
                                           owl:onProperty building:hasMaterial ;
                                           owl:allValuesFrom props:Material
                                         ] ;
                         rdfs:comment "A single and identifiable cross section of an element which is constructed of a number of profiles (one or more)."@en ;
                         rdfs:label "Material Profile" ;
                         rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcMaterialProfile" .


###  http://bimerr.iot.linkeddata.es/def/building#MaterialProfileSet
building:MaterialProfileSet rdf:type owl:Class ;
                            rdfs:subClassOf [ rdf:type owl:Restriction ;
                                              owl:onProperty building:hasMaterialProfile ;
                                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onClass building:MaterialProfile
                                            ] ;
                            rdfs:comment "A designation by which individual material(s) of a prismatic element (for example, beam or column) constructed of a single or multiple material profiles is known."@en ;
                            rdfs:label "Material Profile Set" ;
                            rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcMaterialProfileSet" .


###  http://bimerr.iot.linkeddata.es/def/building#Outlet
building:Outlet rdf:type owl:Class ;
                rdfs:subClassOf building:FlowTerminal ;
                rdfs:comment "An outlet is a device installed at a point to receive one or more inserted plugs for electrical power or communications."@en ;
                rdfs:label "Outlet" ;
                rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcOutlet" .


###  http://bimerr.iot.linkeddata.es/def/building#PhotovoltaicSystem
building:PhotovoltaicSystem rdf:type owl:Class ;
                            rdfs:subClassOf building:System ;
                            rdfs:comment "System that aggregates photovoltaic devices."@en ;
                            rdfs:label "Photovoltaic System" .


###  http://bimerr.iot.linkeddata.es/def/building#PipeSegment
building:PipeSegment rdf:type owl:Class ;
                     rdfs:subClassOf building:FlowSegment ;
                     rdfs:comment "A pipe segment is used to typically join two sections of a piping network."@en ;
                     rdfs:label "Pipe Segment" ;
                     rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcPipeSegment" .


###  http://bimerr.iot.linkeddata.es/def/building#Pump
building:Pump rdf:type owl:Class ;
              rdfs:subClassOf building:FlowMovingDevice ;
              rdfs:comment "A pump is a device which imparts mechanical work on fluids or slurries to move them through a channel or pipeline. A typical use of a pump is to circulate chilled water or heating hot water in a building services distribution system."@en ;
              rdfs:label "Pump" ;
              rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcPump" .


###  http://bimerr.iot.linkeddata.es/def/building#Roof
building:Roof rdf:type owl:Class ;
              rdfs:subClassOf building:BuildingElement ;
              rdfs:comment "A roof is the covering of the top part of a building, it protects the building against the effects of wheather."@en ;
              rdfs:label "Roof" ;
              rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcRoof" .


###  http://bimerr.iot.linkeddata.es/def/building#Slab
building:Slab rdf:type owl:Class ;
              rdfs:subClassOf building:BuildingElement ;
              rdfs:comment "A slab is a component of the construction that normally encloses a space vertically. The slab may provide the lower support (floor) or upper construction (roof slab) in any space in a building."@en ;
              rdfs:label "Slab" ;
              rdfs:seeAlso "http://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL#IfcSlab" .


